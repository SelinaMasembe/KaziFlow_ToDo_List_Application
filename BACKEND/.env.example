# Copy to .env and adjust as needed

# Database connection string
# SQLite (default for local dev)
DATABASE_URL=sqlite:///./aibos.db
# Example Postgres
# DATABASE_URL=postgresql+psycopg://user:pass@host:5432/aibos

# CORS settings (use one of these)
# Explicit allow (recommended)
ALLOWED_ORIGIN=http://localhost:5173
# Or permissive (dev only). Values: true|false
CORS_ALLOW_ALL=false